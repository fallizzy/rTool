"Icon"="C:\\Users\\ezeru\\Desktop\\rTool.exe"

[HKEY_CURRENT_USER\Software\Classes\Directory\shell\rToolImport\command]
@="\"C:\\Users\\ezeru\\Desktop\\rTool.exe\" \"%1\""

[HKEY_CURRENT_USER\Software\Classes\Folder\shell\rToolImport]
@="Import to rTool"
"Icon"="C:\\Users\\ezeru\\Desktop\\rTool.exe"

[HKEY_CURRENT_USER\Software\Classes\Folder\shell\rToolImport\command]
@="\"C:\\Users\\ezeru\\Desktop\\rTool.exe\" \"%1\""
